{
  "name": "df-companion",
  "version": "0.0.1",
  "description": "Dwarf Fortress Companion helps you play Dwarf Fortress",
  "main": "dist/main/index.js",
  "scripts": {
    "test": "npm run lint && npm run mocha",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "start-dev-electron": "cross-env BABEL_ENV=electron electron -r babel-register -r babel-polyfill --inspect=5858 ./src/main",
    "start-dev-server": "cross-env BABEL_ENV=webpack node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./src/webpack/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matthemsteger/df-companion.git"
  },
  "bugs": {
    "url": "https://github.com/matthemsteger/df-companion/issues"
  },
  "keywords": [
    "dwarf fortress"
  ],
  "author": "Matt Hemsteger",
  "license": "MIT",
  "build": {
    "appId": "matthemsteger.df.companion",
    "mac": {
      "category": "public.app-category.simulation-games"
    }
  },
  "dependencies": {
    "@matthemsteger/redux-utils-fn-actions": "^1.0.0",
    "@matthemsteger/redux-utils-fn-constants": "^1.0.0",
    "@matthemsteger/redux-utils-fn-reducers": "^1.0.0",
    "@matthemsteger/redux-utils-fn-selectors": "^1.0.0",
    "@most/core": "^1.5.0",
    "@most/create": "^2.0.1",
    "@most/scheduler": "^1.2.3",
    "@sindresorhus/fnv1a": "^1.2.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.2.1",
    "df-tools": "0.0.1",
    "docuri": "^4.2.2",
    "electron-devtools-installer": "^2.2.4",
    "electron-log": "^3.0.7",
    "fluture": "^11.0.3",
    "folktale": "^2.3.2",
    "font-awesome": "^4.7.0",
    "formik": "^1.5.8",
    "history": "^4.10.1",
    "is-observable": "^2.0.0",
    "joi": "^14.3.1",
    "lodash": "^4.17.15",
    "most": "^1.7.3",
    "most-subject": "^6.0.0",
    "pouchdb": "^7.1.1",
    "pouchdb-adapter-leveldb": "^7.1.1",
    "pouchdb-find": "^7.1.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.13",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-first-router": "^2.1.4",
    "redux-first-router-link": "^2.1.1",
    "redux-most": "^0.8.0",
    "serialize-error": "^4.1.0",
    "shortid": "^2.2.15",
    "styled-components": "^5.0.0-beta.8",
    "to-id": "^2.0.0",
    "uuid": "^3.3.3",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "devtron": "^1.4.0",
    "electron": "^6.0.9",
    "electron-builder": "^21.2.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^4.2.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "proxyquire": "^2.1.3",
    "react-svg-loader": "^3.0.3",
    "sinon": "^7.4.2",
    "sinon-chai": "^3.3.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  }
}
