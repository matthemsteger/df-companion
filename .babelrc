{
	"env": {
		"electron": {
			"presets": [
				["env", {
					"targets": {
						"electron": "2.0.3"
					},
					"useBuiltIns": true
				}],
				"react"
			],
			"plugins": [
				"transform-class-properties",
				"transform-object-rest-spread"
			]
		},
		"webpack": {
			"presets": [
				["env", {
					"targets": {
						"electron": "2.0.3"
					},
					"useBuiltIns": true
				}],
				"react"
			],
			"plugins": [
				"transform-class-properties",
				"transform-object-rest-spread"
			]
		},
		"node": {
			"presets": [
				["env", {
					"targets": {
						"node": "8.9"
					},
					"useBuiltIns": true
				}]
			],
			"plugins": [
				"transform-class-properties",
				"transform-object-rest-spread"
			]
		}
	}
}
